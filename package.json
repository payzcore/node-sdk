{
  "name": "@payzcore/node",
  "version": "1.1.6",
  "description": "Official Node.js SDK for PayzCore blockchain monitoring API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "payzcore",
    "usdt",
    "usdc",
    "trc20",
    "bep20",
    "erc20",
    "polygon",
    "arbitrum",
    "blockchain",
    "monitoring",
    "webhook",
    "stablecoin"
  ],
  "author": "PayzCore",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/payzcore/node-sdk"
  },
  "homepage": "https://docs.payzcore.com",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.5.0"
  }
}
